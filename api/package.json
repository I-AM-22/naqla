{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "nx": "nx",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "yarn build",
    "start:prod": "ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json",
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm  --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "yarn typeorm  migration:create",
    "migration:run": "yarn typeorm  --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "yarn typeorm --dataSource=src/database/data-source.ts migration:revert",
    "redis:queue:up": "docker compose up -d redisqueue",
    "redis:queue:rm": "docker compose rm -s -f -v  redisqueue",
    "redis:queue:restart": "yarn redis:queue:rm && yarn redis:queue:up",
    "db:dev:up": "docker compose up -d dev-db",
    "db:dev:rm": "docker compose rm -s -f -v dev-db",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up",
    "db:rq:up": "yarn db:dev:up && yarn redis:queue:up",
    "db:rq:rm": "yarn db:dev:rm && yarn redis:queue:rm",
    "db:rq:restart": "yarn db:dev:restart && yarn redis:queue:restart",
    "db:create": "ts-node -r dotenv/config ./node_modules/typeorm-extension/bin/cli.cjs db:create  -r src/config/database/postgres --initialDatabase=postgres",
    "db:drop": "ts-node -r dotenv/config ./node_modules/typeorm-extension/bin/cli.cjs db:drop  -r src/config/database/postgres --initialDatabase=postgres",
    "db:seed:fake": "ts-node -r dotenv/config ./node_modules/typeorm-extension/bin/cli.cjs seed:run  -r src/config/database/postgres",
    "db:seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create  --name  src/database/seeders/category/category.seeder.ts",
    "db:seed:real": "ts-node -r tsconfig-paths/register src/seed.ts",
    "db:seed:prod": "ENV=production ts-node -r tsconfig-paths/register src/seed.ts",
    "db:seed": "yarn db:create && yarn db:seed:real && yarn db:seed:fake",
    "doc:generate": "npx compodoc -p tsconfig.doc.json -s",
    "doc:run": "compodoc -s"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "argon2": "^0.31.2",
    "blurhash": "^2.0.5",
    "bull": "^4.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^1.41.2",
    "env-cmd": "^10.1.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "kysely": "^0.27.2",
    "nestjs-telegraf": "^2.7.0",
    "node-schedule": "^2.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.2",
    "telegraf": "^4.15.3",
    "tiny-invariant": "^1.3.1",
    "txtgen": "^3.0.6",
    "typegram": "^5.2.0",
    "typeorm": "^0.3.19",
    "typeorm-extension": "^3.3.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.23",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/passport-jwt": "^4.0.0",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "pactum": "^3.5.1",
    "prettier": "^3.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "resolutions": {
    "string-width": "4.2.3"
  }
}
